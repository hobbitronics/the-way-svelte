{
  "name": "the-way-svelte",
  "version": "1.0.0",
  "scripts": {
    "build:dev": "rollup -c -w",
    "prebuild:prod": "npm run clean:build",
    "build:prod": "rollup -c",
    "clean": "run-p -l clean:build clean:deps",
    "clean:build": "rimraf dist/index.html dist/bundle*",
    "clean:deps": "npm prune",
    "local:dev": "run-p -l build:dev serve",
    "local:prod": "run-p -l build:prod serve",
    "serve": "sirv dist --dev --host 0.0.0.0 --single"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@sveltech/routify": "^1.7.12",
    "npm-run-all": "^4.1.5",
    "qs": "^6.9.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-generate-html-template": "git+ssh://git@github.com:snellcode/rollup-plugin-generate-html-template.git#dist",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sass": "^1.26.7",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.7.4"
  }
}
